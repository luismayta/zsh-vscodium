{
  "cSpell.userWords": [
    "aliexpress",
    "daemonset",
    "fastify",
    "goud",
    "Injectable",
    "kubeadm",
    "kubernetes",
    "nestjs",
    "nrwl",
    "numpy",
    "pytest",
    "Shorttrack"
  ],
  "extensions.autoUpdate": true,
  "extensions.ignoreRecommendations": false,
  "editor.formatOnPaste": true,
  "editor.fontFamily": "Fira Code Retina, Menlo, Monaco, Courier New, monospace",
  "editor.fontSize": 17,
  "editor.lineHeight": 24,
  "editor.fontLigatures": true,
  "editor.fontWeight": "normal",
  "editor.renderWhitespace": "boundary",
  "editor.lineNumbers": "relative",
  "editor.rulers": [80, 120],
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-_",
  "editor.formatOnSave": false,
  "editor.minimap.enabled": false,
  "editor.tabCompletion": "on",
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": true,
    "source.fixAll.tslint": true
  },
  "files.autoSave": "afterDelay",
  "files.associations": {
    ".envrc": "shellscript",
    "zshrc": "shellscript",
    "*.py": "python",
    ".mocharc": "json",
    "Dockerfile.*": "dockerfile",
    "*bashrc": "shellscript",
    "**/bash/**/*": "shellscript",
    "tslint.json": "jsonc"
  },
  "files.exclude": {
    "*_pycache_*": true,
    ".pytest_cache": true,
    ".mypy_cache": true,
    "htmlcov": true,
    "*.egg-info": true,
    "*.pyc": true,
    "**/.git": true,
    "**/.vscode": true,
    ".nyc_output": true,
    "**/__pycache__": true,
    "**/.cache": true,
    "**/.venv": true,
    "**/node_modules": true,
    "**/package-lock.json": true,
    "**/poetry.lock": true,
    "**/yarn.lock": true
  },
  "files.watcherExclude": {
    "**/local/**": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.venv/**": true
  },
  "search.exclude": {
    "*_pycache_*": true,
    "__pycache__": true,
    ".pytest_cache": true,
    ".mypy_cache": true,
    "htmlcov": true,
    "*.egg-info": true,
    ".venv": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "vim.leader": ",",
  "vim.autoindent": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": true,
    "<C-b>": true,
    "<C-j>": false,
    "<C-k>": false,
    "<J>": false,
    "<K>": false,
    "<H>": false
  },
  "vim.iskeyword": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-_",
  "vim.statusBarColorControl": false,
  "vim.searchHighlightColor": "rgba(180, 142, 173, 1)",
  "vim.statusBarColors.insert": "#88c0d0",
  "vim.statusBarColors.normal": "#2e3440",
  "vim.statusBarColors.replace": "#ebcb8b",
  "vim.statusBarColors.visual": "#a3be8c",
  "vim.statusBarColors.visualblock": "#a3be8c",
  "vim.statusBarColors.visualline": "#a3be8c",
  "vim.enableNeovim": false,
  "vim.easymotion": true,
  "vim.easymotionMarkerBackgroundColor": "rgba(40, 40, 40, 0.5)",
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.cursorStylePerMode.insert": "underline",
  "vim.replaceWithRegister": true,
  "vim.scroll": 5,
  "vim.vimrc.enable": false,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": [";"],
      "after": [":"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "m"],
      "commands": [{ "command": "bookmarks.toggle" }]
    },
    {
      "before": ["<leader>", "b"],
      "commands": [{ "command": "bookmarks.list" }]
    },
    {
      "before": ["z", "i"],
      "commands": [{ "command": "workbench.action.zoomIn" }]
    },
    {
      "before": ["z", "o"],
      "commands": [{ "command": "workbench.action.zoomOut" }]
    },
    {
      "before": ["leader", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "s"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["Z", "Z"],
      "commands": [":wq"]
    },
    {
      "before": ["<leader>", "b"],
      "after": [],
      "commands": [
        {
          "command": "open-buffers.openBuffer",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "g", "p"],
      "after": [],
      "commands": [
        {
          "command": "extension.goto-previous-buffer",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "`"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.toggleSidebarVisibility",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "f", "e"],
      "after": [],
      "commands": [
        {
          "command": "extension.file-picker.openFileRelativeToOpenedFile"
        }
      ]
    },
    {
      "before": ["<leader>", "f", "p"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.quickOpen"
        }
      ]
    },
    {
      "before": ["<leader>", "c", "c"],
      "commands": [
        {
          "command": "editor.action.commentLine"
        }
      ]
    },
    {
      "before": ["<leader>", "r"],
      "commands": [
        {
          "command": "workbench.action.reloadWindow"
        }
      ]
    },
    {
      "before": ["<leader>", "/"],
      "commands": [
        {
          "command": "fuzzySearch.activeTextEditor"
        }
      ]
    },
    {
      "before": ["<leader>", "f", "f"],
      "commands": [
        {
          "command": "workbench.action.findInFiles"
        }
      ]
    },
    {
      "before": ["<leader>", "f", "s", "p"],
      "commands": [
        {
          "command": "ghqProjectManager.openProject"
        }
      ]
    },
    {
      "before": ["leader", "q"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.closeActiveEditor",
          "args": []
        }
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["<leader>", "c", "c"],
      "commands": [
        {
          "command": "editor.action.commentLine"
        }
      ]
    },
    {
      "before": ["<leader>", "c", "s"],
      "commands": [
        {
          "command": "editor.action.blockComment"
        }
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["?"],
      "commands": [{ "command": "workbench.action.findInFiles" }]
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    }
  ],
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "type": "tag",
      "foreground": "red"
    },
    "FIXME": {
      "type": "tag",
      "foreground": "#ff00f2"
    },
    "QUESTION": {
      "type": "tag",
      "foreground": "#00f2ff"
    },
    "NOTE": {
      "type": "tag-and-comment",
      "foreground": "#00ff44"
    }
  },
  "todo-tree.general.tags": ["TODO", "FIXME", "QUESTION", "NOTE"],
  "terminal.integrated.fontFamily": "Fira Code Retina, Menlo, Monaco, Courier New, monospace",
  "terminal.integrated.fontSize": 17,
  "terminal.integrated.copyOnSelection": true,
  "window.zoomLevel": 0,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.activityBar.visible": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.colorCustomizations": {
    "tab.unfocusedActiveBorder": "#fff0",
    "statusBar.debuggingBackground": "#2e3440",
    "statusBar.noFolderBackground": "#2e3440",
    "statusBar.background": "#2e3440"
  },
  "terminal.integrated.macOptionIsMeta": true,
  "markdown.preview.fontSize": 16,
  "path-intellisense.autoSlashAfterDirectory": true,
  "explorer.confirmDragAndDrop": false,
  "workbench.iconTheme": "vscode-icons",
  "window.restoreFullscreen": true,
  "go.lintTool": "gometalinter",
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "go.autocompleteUnimportedPackages": true,
  "prettier.arrowParens": "always",
  "explorer.confirmDelete": false,
  "python.pipenvPath": "~/.pyenv/shims/pipenv",
  "python.venvPath": "~/.local/share/virtualenvs/",
  "python.formatting.provider": "black",
  "python.linting.mypyEnabled": true,
  "python.linting.pycodestyleEnabled": true,
  "python.jediEnabled": false,
  "zenMode.hideStatusBar": false,
  "zenMode.centerLayout": false,
  "zenMode.hideLineNumbers": false,
  "zenMode.fullScreen": true,
  "zenMode.hideTabs": false,
  "zenMode.hideActivityBar": false,
  "kite.showWelcomeNotificationOnStartup": false,
  "todo-tree.highlights.defaultHighlight": {
    "type": "tag",
    "background": "#fcf3b3",
    "opacity": 50
  },
  "todo-tree.tree.showScanModeButton": false,
  "editor.accessibilitySupport": "on",
  "workbench.colorTheme": "Atom One Dark",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [80],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false
  },
  "tabnine.experimentalAutoImports": true
}
