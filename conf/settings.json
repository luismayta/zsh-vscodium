{
  "extensions.autoUpdate": true,
  "extensions.ignoreRecommendations": false,

  "editor.fontLigatures": true,
  "editor.formatOnPaste": true,
  "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 16,
  "editor.lineNumbers": "relative",
  "editor.rulers": [80, 120],
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-_",
  "editor.formatOnSave": false,
  "editor.minimap.enabled": false,
  "editor.tabCompletion": true,
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": true,
    "source.fixAll.tslint": true
  },
  "files.autoSave": "afterDelay",
  "files.associations": {
    "*.py": "python",
    ".mocharc": "json",
    "Dockerfile.*": "dockerfile",
    "*bashrc": "shellscript",
    "**/bash/**/*": "shellscript",
    "tslint.json": "jsonc"
  },
  "files.exclude": {
    "*_pycache_*": true,
    ".pytest_cache": true,
    ".mypy_cache": true,
    "htmlcov": true,
    "*.egg-info": true,
    "*.pyc": true,
    "**/.git": true,
    "**/.vscode": true
  },
  "files.watcherExclude": {
    "**/local/**": true
  },
  "search.exclude": {
    "*_pycache_*": true,
    "__pycache__": true,
    ".pytest_cache": true,
    ".mypy_cache": true,
    "htmlcov": true,
    "*.egg-info": true
  },
  "vim.leader": ",",
  "vim.autoindent": true,
  "vim.useSystemClipboard": true,
  "vim.iskeyword": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-_",
  "vim.statusBarColorControl": true,
  "vim.searchHighlightColor": "rgba(180, 142, 173, 1)",
  "vim.statusBarColors.insert": "#88c0d0",
  "vim.statusBarColors.normal": "#2e3440",
  "vim.statusBarColors.replace": "#ebcb8b",
  "vim.statusBarColors.visual": "#a3be8c",
  "vim.statusBarColors.visualblock": "#a3be8c",
  "vim.statusBarColors.visualline": "#a3be8c",
  "vim.easymotionMarkerFontFamily": "Fira Code",
  "vim.enableNeovim": false,
  "vim.easymotion": true,
  "vim.easymotionMarkerFontSize": "16",
  "vim.easymotionMarkerYOffset": 12,
  "vim.easymotionMarkerHeight": 24,
  "vim.easymotionMarkerBackgroundColor": "rgba(40, 40, 40, 0.5)",
  "vim.neovimPath": "/usr/local/bin/nvim",
  "vim.normalModeKeyBindings": [
    {
      "before": [";"],
      "after": [":"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["z", "i"],
      "commands": [{ "command": "workbench.action.zoomIn" }]
    },
    {
      "before": ["z", "o"],
      "commands": [
        { "command": "workbench.action.zoomOut" }
      ]
    },
    {
      "before": ["<leader>", "b"],
      "after": [],
      "commands": [
        {
          "command": "open-buffers.openBuffer",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "g", "p"],
      "after": [],
      "commands": [
        {
          "command": "extension.goto-previous-buffer",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "`"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.toggleSidebarVisibility",
          "args": []
        }
      ]
    },
    {
      "before": ["<leader>", "f", "e"],
      "after": [],
      "commands": [
        {
          "command": "extension.file-picker.openFileRelativeToOpenedFile"
        }
      ]
    },
    {
      "before": ["<leader>", "f", "p"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.quickOpen"
        }
      ]
    },
    {
      "before": ["<leader>", "s"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "c", "c"],
      "commands": [
        {
          "command": "editor.action.commentLine"
        }
      ]
    },
    {
      "before": ["<leader>", "r"],
      "commands": [
        {
          "command": "workbench.action.reloadWindow"
        }
      ]
    },
    {
      "before": ["<leader>", "/"],
      "commands": [
        {
          "command": "fuzzySearch.activeTextEditor"
        }
      ]
    },
    {
      "before": ["<leader>", "f", "f"],
      "commands": [
        {
          "command": "workbench.action.findInFiles"
        }
      ]
    },
    {
      "before": ["<leader>", "f", "s", "p"],
      "commands": [
        {
          "command": "gitProjectManager.openProject"
        }
      ]
    },
    {
      "before": ["leader", "q"],
      "after": [],
      "commands": [
        {
          "command": "workbench.action.closeActiveEditor",
          "args": []
        }
      ]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["<leader>", "c", "c"],
      "commands": [
        {
          "command": "editor.action.commentLine"
        }
      ]
    },
    {
      "before": ["<leader>", "c", "s"],
      "commands": [
        {
          "command": "editor.action.blockComment"
        }
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["?"],
      "commands": [
        { "command": "workbench.action.findInFiles" }
      ]
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    }
  ],
  "gitProjectManager.baseProjectsFolders": ["~/Projects"],
  "gitProjectManager.storeRepositoriesBetweenSessions": true,
  "terminal.integrated.fontFamily": "Fira Code",
  "terminal.integrated.fontSize": 16,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
  "window.zoomLevel": 0,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.activityBar.visible": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.colorCustomizations": {
    "tab.unfocusedActiveBorder": "#fff0",
    "statusBar.debuggingBackground": "#2e3440",
    "statusBar.noFolderBackground": "#2e3440",
    "statusBar.background": "#2e3440"
  },
  "workbench.colorTheme": "Ariake Dark",
  "terminal.integrated.macOptionIsMeta": true,
  "markdown.preview.fontSize": 16,
  "path-intellisense.autoSlashAfterDirectory": true,
  "explorer.confirmDragAndDrop": false,
  "workbench.iconTheme": "vscode-icons",
  "window.restoreFullscreen": true,
  "go.autocompleteUnimportedPackages": true,
  "prettier.arrowParens": "always",
  "todo-tree.highlights.customHighlight": {
    "TODO": {},
    "FIXME": {}
  },
  "todo-tree.highlights.defaultHighlight": {
    "foreground": "green",
    "type": "none"
  },
  "explorer.confirmDelete": false,
  "python.venvPath": "~/.local/share/virtualenvs/",
  "python.linting.mypyEnabled": true,
  "python.linting.pycodestyleEnabled": true,
  "python.jediEnabled": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "zenMode.hideStatusBar": false,
  "zenMode.centerLayout": false,
  "zenMode.hideActivityBar": false,
  "zenMode.hideLineNumbers": false,
  "zenMode.fullScreen": true,
  "zenMode.hideTabs": false,
  "kite.showWelcomeNotificationOnStartup": false,
  "breadcrumbs.enabled": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
