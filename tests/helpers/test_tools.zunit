#!/usr/bin/env zunit

@setup {
    PATH="/bin:/usr/bin:/usr/local/bin"
	  load ../../src/helpers/tools.zsh
}

@teardown {
}

@test 'Load not empty get_path ' {
	  run get_path
	  assert $get_path is_not_empty
}

@test 'Succces path::prepend ' {
	  run path::prepend "hacker"
	  assert $path::prepend is_not_empty
}
