{
  "presets": ["@babel/preset-env", "@babel/preset-typescript"],
  "plugins": [
    ["babel-plugin-root-import"],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@*": ["./"]
        },
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    ],
    "@babel/plugin-proposal-class-properties"
  ]
}
